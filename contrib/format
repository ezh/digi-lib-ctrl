awk '{
  for (i=1; i<=NF; i++) {
    if (i==1) printf$i OFS;printf ""
    if (i==2) printf$i OFS;printf ""
    if (i==3) printf(" P%05d",$i);
    if (i==4) printf(" T%05d",$i);
    if (i==5) printf(" %1s",$i);
    if (i==6) printf(" %-24s ",$i);
    if (i>6) printf$i OFS;printf"";
  }
  print("")
  fflush("")
}' $@